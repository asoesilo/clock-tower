<%= render 'admin/shared/form_error', object: @project %>

<%= form_for @project, url: @path do |f| %>
  <%= f.text_field :name, {class: "form-control", placeholder: "Name"} %>
  <%= f.select :location_id, locations_select_options, { include_blank: "Location" }, { class: "form-control" } %>
  
  <%= f.submit "Submit", class: "btn btn-primary btn-block control-btn" %>
<% end %>