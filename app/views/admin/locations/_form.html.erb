<%= render 'admin/shared/form_error', object: @location %>

<%= form_for [:admin, @location] do |f| %>
  <%= f.text_field :name, {class: "form-control", placeholder: "Name"} %>
  <%= f.text_field :tax_name, {class: "form-control", placeholder: "Tax Name"} %>
  <%= f.text_field :tax_percent, {class: "form-control", placeholder: "Tax %"} %>

  <%= f.submit "Submit", class: "btn btn-primary btn-block control-btn" %>
<% end %>